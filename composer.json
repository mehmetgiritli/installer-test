{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/uofa/uofa-maintenance.git"
        }
    ],
    "require-dev": {
        "fzaninotto/faker": "~1.6",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "require": {
        "php": ">=5.6.4",
        "laravel/lumen-framework": "5.4.*",
        "laravel/passport": "^4.0",
        "predis/predis": "^1.1",
        "uofa/uofa-maintenance": "dev-laravel-5",
        "yajra/laravel-oci8": "^5.4"
    },
    "autoload": {
        "classmap": [

        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]   
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "5.6.40"
        }
    }
}
